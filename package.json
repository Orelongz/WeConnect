  "scripts": {
    "start:dev": "nodemon app.js --exec babel-node",
    "start": "babel-node app.js",
    "lint": "eslint --fix server/ client/src/ app.js",
    "dev-migrate": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "test-migrate": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test && sequelize db:seed:all --env test",
    "prod-migrate": "sequelize db:migrate:undo:all --env production && sequelize db:migrate --env production && sequelize db:seed:all --env production",
    "test:server": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha server/test --timeout 5000 --exit --compilers js:babel-register,js:babel-polyfill",
    "test:client": "jest --coverage",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "nightwatch": "npm run test-migrate && ./node_modules/.bin/nightwatch",
    "nightwatch:run": "cross-env NODE_ENV=test babel-node app.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./client/test/helpers.js",
    "setupFiles": [
      "./client/test/mockData/localStorageMock.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "repository": {
