  "scripts": {
    "start": "babel-node ./server/bin/www",
    "webpack": "webpack-dev-server --config ./webpack.config.js",
    "lint": "eslint --fix server/ client/src app.js webpack.config.js",
    "dev-migrate": "sequelize db:migrate",
    "test-migrate": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "prod-migrate": "sequelize db:migrate:undo:all --env production && sequelize db:migrate --env production",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha server/test --exit --compilers js:babel-register,js:babel-polyfill",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "check-coverage": "nyc check-coverage --lines 70 --per-file",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
